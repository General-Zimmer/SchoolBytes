@model SchoolBytes.Models.FoodModule


@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Index</title>
</head>
<body>
    <h1>Opret et madhold</h1>
    <div>
        @using (Html.BeginForm("CreateFoodModule", "Food"))
        {
            @Html.EditorForModel()

            <br />
            @Html.LabelFor(fm => fm.Course)
            @(": ")
            @Html.DisplayFor(fm => fm.Course.Name)


            <br />
            @Html.LabelFor(fm => fm.Teacher)
            @(": ")
            @Html.DisplayFor(fm => fm.Teacher.Name)

            <br />
            <input type="submit" value="Opret" />
        }

    </div>
</body>
</html>
